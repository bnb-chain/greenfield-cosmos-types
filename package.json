{
  "name": "@bnb-chain/greenfield-cosmos-types",
  "version": "0.4.0-alpha.30",
  "description": "greenfield cosmos types",
  "scripts": {
    "codegen": "rimraf ./src && node ./scripts/codegen.js",
    "convert": "rimraf ./eip712 && node scripts/convert.js",
    "clean": "rimraf amino confio cosmos cosmos_proto gogoproto google greenfield tendermint index.* helpers.* varint.* utf8.* binary.*",
    "prepare-publishing": "cp -r dist/* .",
    "build": "tsc"
  },
  "keywords": [
    "cosmos",
    "greenfield",
    "types",
    "typescript",
    "gRPC"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "amino/",
    "confio/",
    "cosmos/",
    "cosmos_proto/",
    "gogoproto/",
    "google/",
    "greenfield/",
    "tendermint/",
    "eip712/",
    "/index.*",
    "/helpers.*",
    "/utf8.*",
    "/varint.*",
    "/binary.*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@cosmology/telescope": "1.3.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/long": "^4.0.2",
    "@types/node": "^15.14.9",
    "rimraf": "^4.4.1",
    "semantic-release": "^19.0.5",
    "ts-morph": "^18.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "long": "^4.0.0",
    "protobufjs": "~6.11.4"
  },
  "sideEffects": false
}
