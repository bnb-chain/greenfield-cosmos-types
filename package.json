{
  "name": "@bnb-chain/greenfield-cosmos-types",
  "version": "0.4.0-alpha.8",
  "description": "greenfield cosmos types",
  "scripts": {
    "codegen": "rimraf ./src && node ./scripts/codegen.js",
    "convert": "rimraf ./eip712 && node scripts/convert.js",
    "clean": "rimraf confio cosmos cosmos_proto gogoproto google greenfield tendermint index.* helpers.*",
    "prepare-publishing": "cp -r dist/* .",
    "build": "tsc"
  },
  "keywords": [
    "cosmos",
    "greenfield",
    "types",
    "typescript",
    "gRPC"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "confio/",
    "cosmos/",
    "cosmos_proto/",
    "gogoproto/",
    "google/",
    "greenfield/",
    "tendermint/",
    "eip712/",
    "/index.*",
    "/helpers.*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@osmonauts/telescope": "^0.81.5",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/long": "^4.0.1",
    "@types/node": "^15.6.2",
    "rimraf": "^4.4.0",
    "semantic-release": "^19.0.3",
    "ts-morph": "^18.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "long": "^4.0.0",
    "protobufjs": "~6.11.2"
  }
}
